<ItemTickets>:
    img_lef:img_lef
    ImageLeftWidget:
        id : img_lef
        source: './img_profile/default-image.png'

<CardSubscription>:
    list_tickets: list_tickets
    title : title
    titleSub: titleSub
    orientation: "vertical"
    padding: "8dp"
    size_hint: None, 1
    size: "380dp", "1280dp"
    BoxLayout:
        orientation: "vertical"
        size_hint:1,1
        BoxLayout:
            orientation: "horizontal"
            size_hint_y: None
            height: 60
            canvas.before:
                Color:
                    rgb: app.theme_cls.primary_color
                RoundedRectangle:
                    pos: self.pos
                    size: self.size
                    radius:[1,1,0,0]
            BoxLayout:
                padding:10
                spacing:5
                orientation: "vertical"
                size_hint_x : .9
                MDLabel:
                    id: titleSub
                    size_hint_x: None
                    width:100
                    font_style: 'Subtitle1'
                    text: 'Subscription'
                    theme_text_color: "Secondary"
                MDLabel:
                    id: title
                    font_size: '15sp'
                    theme_text_color: "Custom"
                    text_color: 1,1,1,1
            AnchorLayout:
                size_hint_x : .1
                anchor_x: "right"
                anchor_y: "top"
                MDIconButton:
                    icon: "cog"
                    user_font_size: "24sp"
        ScrollView:
            MDList:
                id : list_tickets






<Afterlogin>:
    scroll_view : container
    navDrawer : navDrawer
    screenManager : screenManager
    list_message : list_message
    text_to_sent : text_to_sent
    NavigationLayout:

        ScreenManager:
            id: screenManager
            Screen:
                id : screen1
                name: 'subscriptions'
                MDBoxLayout:
                    orientation:'vertical'
                    MDToolbar:
                        title: "Entity"
                        elevation: 10
                        left_action_items: [['menu', lambda x: navDrawer.set_state()]]
                    MDBoxLayout:
                        orientation:'vertical'

                        ScrollView:
                            size: self.size

                            GridLayout:
                                id: container
                                padding: [10,10,10,10]
                                spacing: 10
                                size_hint_x: None
                                #row_default_height: root.height*0.1
                                #height: self.minimum_height
                                width: self.minimum_width
            Screen:
                id : screen2
                name: 'message'
                MDBoxLayout:
                    orientation:'vertical'
                    MDToolbar:
                        title: "Entity"
                        elevation: 10
                        left_action_items: [['menu', lambda x: navDrawer.set_state()]]
                        right_action_items: [["dots-vertical",  lambda x : root.back_subscriptions()], ["arrow-left", lambda x : root.back_subscriptions() ]]

                    MDBoxLayout:
                        padding:20
                        orientation:'vertical'
                        spacing: 10
                        MDBoxLayout:
                            size_hint_y : .86
                            orientation:'vertical'
                            spacing: 10
                            ScrollView:
                                id: list_message
                                size_hint: 1, 1
                                size: self.size

                        MDBoxLayout:
                            orientation:'horizontal'
                            size_hint_y : .14
                            spacing: 10
                            canvas.before:
                                Color:
                                    rgba: .011, .27, 87, .10
                                RoundedRectangle:
                                    pos: self.pos
                                    size: self.size[0] , 55
                                    radius: [25,]
                            MDLabel:
                                text: " "
                                size_hint: .04 ,None
                            MDTextField:
                                multiline: True
                                id: text_to_sent
                                write_tab: True
                            MDFloatingActionButton:
                                icon: "send-outline"
                                user_font_size: "32sp"
                                theme_text_color: "Custom"
                                text_color: 1,1,1,1
                                md_bg_color: app.theme_cls.primary_color
                                on_release:root.sent_message()


        MDNavigationDrawer:
            id: navDrawer
            avatar: avatar
            namePerson: namePerson
            idName: idName
            email: email
            open_file:open_file
            orientation: "vertical"

            Widget:
                size_hint_y: None
                height: 8
            AnchorLayout:
                anchor_x: "left"
                anchor_y: "top"
                BoxLayout:
                    orientation: "vertical"

                    padding: "8dp"
                    spacing: "8dp"
                    size_hint_y: None
                    height:350

                    AnchorLayout:
                        anchor_x: "left"
                        size_hint_y: None

                    RelativeLayout:
                        Image:
                            id: avatar
                            size_hint: None, None
                            pos: 0, 0
                        MDFloatingActionButton:
                            id:open_file
                            pos: 88, -8
                            icon: "pencil"
                            user_font_size: "14sp"
                            theme_text_color: "Custom"
                            text_color: 1,1,1,1
                            md_bg_color: app.theme_cls.primary_color
                    MDLabel:
                        id: namePerson
                        font_style: "Button"
                        size_hint_y: None
                        height: self.texture_size[1]
                    BoxLayout:
                        orientation: "horizontal"
                        size_hint_y: None
                        height: LabelAccount.texture_size[1]
                        MDLabel:
                            id:LabelAccount
                            text: "Cuenta: "
                            font_style: "Subtitle2"
                            size_hint_y: None
                            height: self.texture_size[1]
                            size_hint_x: None
                            width:55

                        MDLabel:
                            id:idName
                            font_style: "Caption"
                            size_hint_y: None
                            height: self.texture_size[1]

                    BoxLayout:
                        orientation: "horizontal"
                        size_hint_y: None
                        height: LabelEmail.texture_size[1]
                        MDLabel:
                            id:LabelEmail
                            text: "Email: "
                            font_style: "Subtitle2"
                            size_hint_y: None
                            height: self.texture_size[1]
                            size_hint_x: None
                            width:55
                        MDLabel:
                            id:email
                            font_style: "Caption"
                            size_hint_y: None
                            height: self.texture_size[1]

                    MDList:
                        OneLineListItem:
                            text: "Mis subcripciones"
                            on_press:

                        OneLineListItem:
                            text: "Entidades"
                            on_press:

                        OneLineListItem:
                            text: "Miembros"
                            on_press:



